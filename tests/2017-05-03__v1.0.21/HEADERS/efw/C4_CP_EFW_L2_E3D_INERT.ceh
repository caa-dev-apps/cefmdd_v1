START_META     =   DATASET_ID
   ENTRY       =   "C4_CP_EFW_L2_E3D_INERT"
END_META       =   DATASET_ID

START_META     =   DATA_TYPE
   ENTRY       =   "CP>CAA_Parameter"
END_META       =   DATA_TYPE

START_META     =   DATASET_TITLE
   ENTRY       =   "Full 3D Electric field in ISR2 with vxB removed"
END_META       =   DATASET_TITLE

START_META     =   DATASET_DESCRIPTION
   ENTRY       =   "This dataset contains measurements of the Electric field in the inertial frame "
   ENTRY       =   "(i.e., vxB removed) in the spacecraft coordinate system (ISR2), using EFW "
   ENTRY       =   "electric field data from file C4_CP_EFW_L2_E and interpolated FGM "
   ENTRY       =   "magnetic field data from C4_CP_FGM_FULL. The spin-axis component "
   ENTRY       =   "of the electric field is calculated with assumption of E.B equals 0 and "
   ENTRY       =   "with use of 2 electric field components in the spin plane and "
   ENTRY       =   "three magnetic field components."
END_META       =   DATASET_DESCRIPTION

START_META     =   CONTACT_COORDINATES
   ENTRY       =   "caateam>Non-PI>caateam@rssd.esa.int"
END_META       =   CONTACT_COORDINATES

