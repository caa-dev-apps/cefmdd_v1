END_META       =   METADATA_VERSION

START_VARIABLE  =  time_tags__C3_PP_EFW
    PARAMETER_TYPE      = "Support_Data"
    CATDESC             = "Interval centred time tag"
    UNITS               = "s"
    SI_CONVERSION       = "1.0>s"
    SIZES               = 1
    VALUE_TYPE          = ISO_TIME
    SIGNIFICANT_DIGITS  = 24
    FILLVAL             = 9999-12-31T23:59:59Z
    FIELDNAM            = "Universal Time"
    LABLAXIS            = "UT"
    DELTA_PLUS          = half_interval__C3_PP_EFW
    DELTA_MINUS         = half_interval__C3_PP_EFW
END_VARIABLE    =  time_tags__C3_PP_EFW

START_VARIABLE  =  half_interval__C3_PP_EFW
    PARAMETER_TYPE      = "Support_Data"
