    SIZES               = 1
    VALUE_TYPE          = FLOAT
    SIGNIFICANT_DIGITS  = 6
    FILLVAL             = -1.0e28
    FIELDNAM            = "Half width of averaging interval"
    LABLAXIS            = "s"
    DATA                = 2.0
END_VARIABLE    =  half_interval__C3_PP_EFW

