END_VARIABLE         = duration__C4_CP_CIS-CODIF_HS_H1_MOMENTS

START_VARIABLE       = density__C4_CP_CIS-CODIF_HS_H1_MOMENTS
  CATDESC            = "Density (in cm^-3)"
