  FIELDNAM           = "Center time"
  PARAMETER_TYPE     = "Support_Data"
  VALUE_TYPE         = ISO_TIME
  UNITS              = "s"
  SI_CONVERSION      = "1.0>s"
  LABLAXIS           = "UT"
  DELTA_PLUS         = duration__C4_CP_CIS-CODIF_HS_H1_MOMENTS
  DELTA_MINUS        = duration__C4_CP_CIS-CODIF_HS_H1_MOMENTS
