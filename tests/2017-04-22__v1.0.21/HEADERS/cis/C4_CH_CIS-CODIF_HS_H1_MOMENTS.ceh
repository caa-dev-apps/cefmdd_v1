  UNITS              = "particles cm^-3"
  SI_CONVERSION      = "1e6>(particles) m^-3"
  DEPEND_0           = time_tags__C4_CP_CIS-CODIF_HS_H1_MOMENTS
  QUALITY            = 3
  SIGNIFICANT_DIGITS = 4
  FILLVAL            = -1e31
