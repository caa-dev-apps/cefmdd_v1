  VALUE_TYPE         = FLOAT
  UNITS              = "s"
  SI_CONVERSION      = "1.0>s"
  LABLAXIS           = "dT"
  DEPEND_0           = time_tags__C4_CP_CIS-CODIF_HS_H1_MOMENTS
